<?xml version="1.0" encoding='UTF-8'?>
<config>
	<!-- 營運階段用 -->
	<!-- 語系 -->
	<localeHelper>
		<locale>zh_TW</locale>
	</localeHelper>

	<!-- 時區 -->
	<timeZoneHelper>
		<timeZone>GMT+8</timeZone>
	</timeZoneHelper>

	<!-- 設定 -->
	<configHelper>
		<!-- checksum -->
		<checksum>true</checksum>
		<checksumType>ADLER32</checksumType>
		<checksumKey>LongLongAgo</checksumKey>
		
		<!-- serialize -->
		<serialize>true</serialize>
		<serializeType>KRYO</serializeType>
		
		<!-- security -->
		<security>true</security>
		<securityType>DES_ECB_PKCS5Padding</securityType>
		<securityKey>NotFarAway</securityKey>
		
		<!-- compress -->
		<compress>true</compress>
		<compressType>LZ4</compressType>
	</configHelper>
	
	<!-- vip -->
	<vipCollector>
		<!-- vip等級 
		<vipType>
			<entry>
				<key>1</key>
				<value>10</value>
			</entry>
			<entry>
				<key>2</key>
				<value>100</value>
			</entry>
			<entry>
				<key>3</key>
				<value>500</value>
			</entry>
			<entry>
				<key>4</key>
				<value>1000</value>
			</entry>
			<entry>
				<key>5</key>
				<value>2000</value>
			</entry>
			<entry>
				<key>6</key>
				<value>5000</value>
			</entry>
			<entry>
				<key>7</key>
				<value>10000</value>
			</entry>
			<entry>
				<key>8</key>
				<value>20000</value>
			</entry>
			<entry>
				<key>9</key>
				<value>50000</value>
			</entry>
			<entry>
				<key>10</key>
				<value>100000</value>
			</entry>
		</vipType>
		-->
		<vipType>
			<entry>
				<key>1</key>
				<value>1000</value>
			</entry>
			<entry>
				<key>2</key>
				<value>5000</value>
			</entry>
			<entry>
				<key>3</key>
				<value>10000</value>
			</entry>
			<entry>
